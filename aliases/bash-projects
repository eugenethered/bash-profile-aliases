# bash related commands for specific projects

# banker
alias banker.lxc.start="lxc.start-container banker-browser-ext"
alias banker.lxc.stop="lxc.stop-container banker-browser-ext"
alias banker.lxc.run-in="lxc.run-in.container banker-browser-ext"
alias banker.project="cd ~/projects/code/banker/banker-browser-extension"

